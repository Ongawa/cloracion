<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.collections.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>


<AnchorPane prefHeight="500.0" prefWidth="690.0"
    xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/8">
    <children>
        <GridPane layoutX="14.0" layoutY="14.0" prefHeight="167.0"
            prefWidth="675.0">
            <columnConstraints>
                <ColumnConstraints hgrow="SOMETIMES"
                    prefWidth="10.0" />
                <ColumnConstraints hgrow="SOMETIMES"
                    prefWidth="100" />
                <ColumnConstraints hgrow="SOMETIMES"
                    prefWidth="150.0" />
                <ColumnConstraints hgrow="SOMETIMES"
                    prefWidth="30.0" />
                <ColumnConstraints hgrow="SOMETIMES"
                    prefWidth="100.0" />
                <ColumnConstraints hgrow="SOMETIMES"
                    prefWidth="150.0" />
                <ColumnConstraints hgrow="SOMETIMES"
                    prefWidth="10.0" />
            </columnConstraints>
            <rowConstraints>
                <RowConstraints minHeight="10.0"
                    prefHeight="30.0" vgrow="SOMETIMES" />
                <RowConstraints minHeight="10.0"
                    prefHeight="30.0" vgrow="SOMETIMES" />
                <RowConstraints minHeight="10.0"
                    prefHeight="30.0" vgrow="SOMETIMES" />
                <RowConstraints minHeight="10.0"
                    prefHeight="30.0" vgrow="SOMETIMES" />
            </rowConstraints>
            <children>
                <Label alignment="CENTER" prefHeight="43.0"
                    prefWidth="135.0" text="Tipo" textAlignment="CENTER"
                    GridPane.columnIndex="1" />
                <Label alignment="CENTER" prefHeight="43.0"
                    prefWidth="135.0" text="Tuberia" textAlignment="CENTER"
                    GridPane.columnIndex="2" fx:id="elementType"/>
                
                <Label alignment="CENTER" prefHeight="43.0"
                    prefWidth="135.0" text="Nombre" textAlignment="CENTER"
                    GridPane.columnIndex="1" GridPane.rowIndex="1" />
               <TextField GridPane.columnIndex="2"
                    GridPane.rowIndex="1" fx:id="elementName" 
                    prefWidth="135.0" />
                
                    
                <Label alignment="CENTER" prefHeight="42.0"
                    prefWidth="113.0" text="Concentracion"
                    textAlignment="CENTER" GridPane.columnIndex="4" />
                <TextField GridPane.columnIndex="5" fx:id="clConcetration" 
                    prefWidth="135.0" />
                   
                    
                <Label alignment="CENTER" prefHeight="50.0"
                    prefWidth="135.0" text="Longitud"
                    GridPane.columnIndex="1" GridPane.rowIndex="2" />
                <TextField GridPane.columnIndex="2"
                    GridPane.rowIndex="2" fx:id="elementLenght"      prefWidth="135.0" />
                    
                <Label alignment="CENTER" prefHeight="50.0"
                    prefWidth="135.0" text="Diametro"
                    GridPane.columnIndex="1" GridPane.rowIndex="3" />
                <TextField GridPane.columnIndex="2"
                    GridPane.rowIndex="3" fx:id="elementDiameter" 
                    prefWidth="135.0" />
                
                <Label alignment="CENTER" prefHeight="42.0"
                    prefWidth="141.0" text="Tiempo Retencion"
                    textAlignment="CENTER" GridPane.columnIndex="4"
                    GridPane.rowIndex="1" />
                <TextField GridPane.columnIndex="5"
                    GridPane.rowIndex="1" fx:id="retentionTime" 
                    prefWidth="135.0" />
                    
                <Button alignment="CENTER" contentDisplay="CENTER"
                    mnemonicParsing="false" prefHeight="25.0" prefWidth="98.0"
                    text="Guardar" textAlignment="CENTER"
                    GridPane.columnIndex="5" GridPane.rowIndex="3" fx:id="saveData" onAction="#saveCurrentData"/>
            </children>
        </GridPane>
    </children>
</AnchorPane>
